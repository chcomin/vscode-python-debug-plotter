{
  "name": "python-debug-plotter",
  "displayName": "Python Debug Plotter: Arrays, Tensors & Graphs",
  "description": "Debug and plot NumPy arrays, PyTorch tensors, images, and NetworkX graphs instantly. Lightweight & dependency-free.",
  "version": "0.0.2",
  "publisher": "inag-ufscar",
  "author": {
    "name": "Cesar Comin"
  },
  "icon": "assets/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chcomin/vscode-python-debug-plotter"
  },
  "bugs": {
    "url": "https://github.com/chcomin/vscode-python-debug-plotter/issues"
  },
  "homepage": "https://github.com/chcomin/vscode-python-debug-plotter#readme",
  "keywords": [
    "python",
    "debug",
    "visualization",
    "data viewer",
    "numpy",
    "pytorch",
    "tensor",
    "array",
    "networkx",
    "debugger",
    "plot",
    "image viewer",
    "graph",
    "point cloud"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Visualization",
    "Debuggers",
    "Data Science"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-debug-plotter.plotVariable",
        "title": "Plot variable"
      }
    ],
    "menus": {
      "debug/variables/context": [
        {
          "command": "python-debug-plotter.plotVariable",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "plotly.js": "^3.3.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
